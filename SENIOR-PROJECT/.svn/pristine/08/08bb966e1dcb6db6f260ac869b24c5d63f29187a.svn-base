    //When mouse is removed  
    $(".blue").mouseout(function(){  
        $(this).stop().animate({height:'40px'},{queue:false, duration:600, easing: 'easeOutBounce'})  
    });  
  
    $("#slider").easySlider({
		auto: true,
		continuous: true,
		controlsShow: true,
		numeric:true
	});

    $("#marquee").marquee(); 
});  